<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:self="clr-namespace:T3WPFGui"
        x:Class="T3WPFGui.GameSelectWindow" 
        Title="GameSelectWindow" Height="300" Width="300">
    <Window.CommandBindings>
        <CommandBinding Command="self:Commands.AIGame" Executed="NewAIGame"/>
        <CommandBinding Command="self:Commands.HostGame" Executed="NewHostGame"/>
        <CommandBinding Command="self:Commands.JoinGame" Executed="NewJoinGame"/>
    </Window.CommandBindings>
    <StackPanel>
        <Button Content="AI Game"  Command="self:Commands.AIGame" />
        <Button Content="Host Game" Command="self:Commands.HostGame" />
        <Button Content="Join Game" Command="self:Commands.JoinGame" />

    </StackPanel>
</Window>
